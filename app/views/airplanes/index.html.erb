<p id="notice"><%= notice %></p>

<h1>Airplanes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @airplanes.each do |airplane| %>
      <li>
        <b> <%=  airplane.name %> </b> ROW <b><%= airplane.row %></b> COLUMN <b><%= airplane.column %></b><%= link_to ' Show ', airplane %><%= link_to 'Edit ', edit_airplane_path(airplane) %><%= link_to 'Destroy', airplane, method: :delete, data: { confirm: 'Are you sure?' } %></li>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Airplane', new_airplane_path %>
